{
    "name": "magicspacepanda/time-duration",
    "description": "Convert durations into human-readable timestamps.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "time",
        "interval",
        "duration"
    ],
    "authors": [
        {
            "name": "Nathanael McDaniel",
            "email": "leemcd56@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "MagicSpacePanda\\": "src/TimeDuration"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --verbose"
    },
    "require": {
        "php": "^7.2 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    }
}
